<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo" xmlns:dobbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
            http://code.alibabatech.com/schema/dubbo
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="${dubbo.application.name}" />
    <dubbo:registry address="zookeeper://${dubbo.address}" />
    <dubbo:protocol name="dubbo" serialization="${dubbo.serialization}" port="${dubbo.port}" threadpool="cached" />
    <dubbo:annotation package="com.yylc.risk.gateway.business.service.impl" />
    <!-- 生成远程服务代理，可以和本地bean一样使用demoService-->
    <!--data-->
    <dubbo:reference id="dataDirectService" interface="com.yylc.risk.data.facade.api.IDataDirectService" protocol="dubbo"   check="false"/>
    <dubbo:reference id="dataQueryService" interface="com.yylc.risk.data.facade.api.IDataQueryService" protocol="dubbo"  check="false"/>
    <dubbo:reference id="dataEventService" interface="com.yylc.risk.data.facade.api.IDataEventService" protocol="dubbo"  check="false"/>
    <dubbo:reference id="dataRiskBlackListService" interface="com.yylc.risk.data.facade.api.IDataRiskBlackListService" protocol="dubbo"  check="false"/>


</beans>